[Unit]
Description=Telegram bot server
After=network.target
StartLimitIntervalSec=0
[Service]
Type=simple
Restart=always
RestartSec=1
User=mohamed
ExecStart= /home/mohamed/.cargo/bin/cargo run --release /home/mohamed/personal/homebot/Cargo.toml
WorkingDirectory=/home/mohamed/personal/homebot

[Install]
WantedBy=multi-user.target
